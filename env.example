# Environment Configuration
MODE=dev

# Authentication Credentials
GOOGLE_EMAIL=your.test@email.com
GOOGLE_PASSWORD=your-test-password

# Staging Environment (optional)
STAGING_GOOGLE_EMAIL=staging.test@email.com
STAGING_GOOGLE_PASSWORD=staging-test-password

# Production Environment (optional)
PROD_GOOGLE_EMAIL=prod.test@email.com
PROD_GOOGLE_PASSWORD=prod-test-password

# API Configuration
API_TOKEN=your-api-token-here

# Test Configuration
HEADLESS=0
FORCE_AUTH=false

# Browser Configuration
BROWSER=chromium
VIEWPORT_WIDTH=1280
